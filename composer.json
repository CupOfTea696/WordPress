{
    "name": "cupoftea/wordpress",
    "description": "WordPress",
    "type": "cupoftea-wordpress",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Wittevrongel",
            "email": "cupoftea696@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "cupoftea/package": "^1.3",
        "cupoftea/wordpress-composer": "^1.0",
        "cupoftea/wordpress-lib": "^1.0",
        "johnpbloch/wordpress": "^4.7"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CupOfTea\\": ""
        }
    },
    "extra": {
        "public-dir": "public",
        "main": "app.php",
        "files": {
            "app.php": false,
            "bootstrap/": true,
            "bootstrap/ensure-correct-url.php": false,
            "bootstrap/set-theme-dir.php": false,
            "config/": true,
            "config/app.php": false,
            "config/view.php": false,
            "public/": true,
            "public/.htaccess": true,
            "public/assets/": true,
            "public/assets/.gitkeep": true,
            "public/content/": true,
            "public/content/index.php": true,
            "public/content/themes/": true,
            "public/content/themes/index.php": true,
            "public/index.php": true,
            "public/plugins/": true,
            "public/plugins/index.php": true,
            "public/themes/": true,
            "public/themes/index.php": true,
            "public/wp-config.php": true,
            "storage/": true,
            "storage/app/": true,
            "storage/app/.gitignore": true,
            "storage/app/public/": true,
            "storage/app/public/.gitignore": true,
            "storage/framework/": true,
            "storage/framework/.gitignore": true,
            "storage/framework/views/": true,
            "storage/framework/views/.gitignore": true,
            "storage/logs/": true,
            "storage/logs/.gitignore": true
        }
    }
}
